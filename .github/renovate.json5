{
  extends: [
    'github>JavierSegoviaCordoba/renovate//renovate.json5',
  ],
  packageRules: [
    {
      matchUpdateTypes: [
        'major',
        'minor',
        'patch',
      ],
      groupName: 'all dependencies',
      minimumReleaseAge: '6 hours',
      matchPackageNames: [
        '*',
      ],
    },
    {
      groupName: 'Kotlin, and compiler related dependencies',
      minimumReleaseAge: '6 hours',
      matchPackageNames: [
        'androidx.compose.compiler',
        'app.cash.molecule',
        'com.javiersc.kotlin.*',
        'com.javiersc.kotlin:kotlin-*',
        'org.jetbrains.compose.compiler',
        'org.jetbrains.kotlin.*',
        'org.jetbrains.kotlin:kotlin-*',
      ],
    },
    {
      groupName: 'Hubdle dependencies',
      minimumReleaseAge: '6 hours',
      matchPackageNames: [
        'com.javiersc.hubdle',
        'com.javiersc.hubdle.*',
        'com.javiersc.hubdle:',
        'com.javiersc.hubdle:hubdle',
        'com.javiersc.hubdle:hubdle.',
        'com.javiersc.hubdle:hubdle.*',
        'com.javiersc.hubdle:hubdle-',
        'com.javiersc.hubdle:hubdle-*'
      ],
    },
    {
      groupName: 'Sqldelight dependencies',
      minimumReleaseAge: '6 hours',
      matchPackageNames: [
        'app.cash.sqldelight',
        'app.cash.sqldelight.*',
        'app.cash.sqldelight:*'
      ],
    },
    {
      "matchPackageNames": [
        "org.junit.**",
        "org.junit**",
        "org.junit.jupiter.**",
        "org.junit.jupiter**",
        "org.junit.platfor.**",
        "org.junit.platfor**"
      ],
      "enabled": false
    }
  ],
  prConcurrentLimit: 2,
}
